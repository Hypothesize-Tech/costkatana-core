{
  "name": "ai-cost-tracker",
  "version": "1.1.0",
  "description": "Track, analyze, and optimize AI API costs across multiple providers with intelligent gateway, caching, retries, workflow tracking, secure proxy key authentication, and prompt firewall protection",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "clean": "rm -rf dist",
    "prebuild": "npm run clean",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint 'src/**/*.ts'",
    "lint:fix": "eslint 'src/**/*.ts' --fix",
    "format": "prettier --write 'src/**/*.ts'",
    "prepublishOnly": "npm run test && npm run build",
    "prepare": "npm run build",
    "dev": "tsc -w"
  },
  "keywords": [
    "ai",
    "cost-tracking",
    "openai",
    "aws-bedrock",
    "api-optimization",
    "prompt-engineering",
    "token-counting",
    "gpt",
    "claude",
    "typescript",
    "ai-gateway",
    "proxy",
    "caching",
    "retries",
    "workflow-tracking",
    "anthropic",
    "google-ai",
    "cohere",
    "proxy-keys",
    "key-vault",
    "secure-authentication",
    "budget-control",
    "access-control",
    "prompt-firewall",
    "security-shield",
    "threat-detection",
    "prompt-injection",
    "llama-guard",
    "prompt-guard",
    "cost-shield"
  ],
  "author": "Abdul Sagheer",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Hypothesize-Tech/ai-cost-optimizer-core.git"
  },
  "bugs": {
    "url": "https://github.com/Hypothesize-Tech/ai-cost-optimizer-core/issues"
  },
  "homepage": "https://github.com/Hypothesize-Tech/ai-cost-optimizer-core#readme",
  "engines": {
    "node": ">=14.0.0"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "dependencies": {
    "@aws-sdk/client-bedrock-runtime": "^3.450.0",
    "@aws-sdk/credential-providers": "^3.450.0",
    "ai-cost-tracker": "^1.0.7",
    "axios": "^1.10.0",
    "dotenv": "^16.5.0",
    "gpt-3-encoder": "^1.1.4",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "^20.10.5",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.2",
    "jest": "^29.7.0",
    "prettier": "^3.1.1",
    "ts-jest": "^29.1.1",
    "typescript": "^5.3.3"
  }
}
